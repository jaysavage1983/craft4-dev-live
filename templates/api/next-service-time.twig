{% for entry in craft.entries.section('live') %}
{
	"nextTime":"{{ entry.schedule.loopStartDate | date('h:iA l') }}",
	"nextDay":"{{ entry.schedule.loopStartDate | date('F d, Y') }}",
	"year":"{{ entry.schedule.loopStartDate | date('Y') }}",
	"month":"{{ entry.schedule.loopStartDate | date('m') }}",
	"day":"{{ entry.schedule.loopStartDate | date('d') }}",
	"start_hour":"{{ entry.schedule.loopStartDate | date('H') }}",
	"start_minute":"{{ entry.schedule.loopStartDate | date('i') }}",
	"end_day":"{{ entry.schedule.loopEndDate | date('w') }}",
	"end_hour":"{{ entry.schedule.loopEndDate | date('H') }}",
	"end_minute":"{{ entry.schedule.loopEndDate | date('i') }}"
}
{% endfor %}